        <activity android:name="com.digitalhomeland.organizer.DashboardActivity" >

        </activity>
        <activity android:name="com.digitalhomeland.organizer.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.digitalhomeland.organizer.SignupActivity" />
        <activity android:name="com.digitalhomeland.organizer.UnapprovedView" />
